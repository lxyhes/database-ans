{
  "name": "data-analysis-assistant",
  "version": "2.0.0",
  "description": "A desktop application for natural language data analysis queries - Vue 3 + Vite + Electron",
  "main": "dist-electron/main.js",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "electron:dev": "vite",
    "electron:build": "vue-tsc --noEmit && vite build && electron-builder",
    "electron:preview": "vite build && electron ."
  },
  "keywords": [
    "data",
    "analysis",
    "natural-language",
    "desktop",
    "vue3",
    "electron"
  ],
  "author": "Data Analysis Assistant",
  "license": "MIT",
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "autoprefixer": "^10.4.24",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.0",
    "postcss": "^8.5.6",
    "sass-embedded": "^1.97.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-electron": "^0.28.0",
    "vite-plugin-electron-renderer": "^0.14.0",
    "vue-tsc": "^1.8.0"
  },
  "dependencies": {
    "@arco-design/web-vue": "^2.57.0",
    "@element-plus/icons-vue": "^2.3.0",
    "axios": "^1.6.0",
    "echarts": "^5.4.3",
    "highlight.js": "^11.11.1",
    "markdown-it": "^14.1.0",
    "pinia": "^2.1.0",
    "vue": "^3.4.0",
    "vue-echarts": "^6.6.0",
    "vue-router": "^4.2.0"
  }
}
